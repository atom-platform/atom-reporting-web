{
  "name": "@atom-platform/ngx-charts-web",
  "version": "0.1.12",
  "scripts": {
    "ng": "ng",
    "start": "npm run build-lib && ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-lib": "ng build --prod ngx-charts-web",
    "npm-pack": "cd dist/ngx-charts-web && npm pack",
    "package": "npm run build-lib && npm run npm-pack",
    "version-major": "cd projects/ngx-charts-web && npm version major && cd ../.. && git add . && git commit -m 'updating lib version' && npm version major",
    "version-minor": "cd projects/ngx-charts-web && npm version minor && cd ../.. && git add . && git commit -m 'updating lib version' && npm version minor",
    "version-patch": "cd projects/ngx-charts-web && npm version patch && cd ../.. && git add . && git commit -m 'updating lib version' && npm version patch",
    "publish": "npm publish dist/ngx-charts-web --access public"
  },
  "private": false,
  "dependencies": {
    "@angular/common": "7.0.1",
    "@angular/core": "7.0.1",
    "@angular/compiler": "^7.2.14",
    "@angular/animations": "7.0.1",
    "@angular/forms": "7.0.1",
    "@angular/platform-browser": "7.0.1",
    "@angular/platform-browser-dynamic": "7.0.1",
    "@angular/router": "7.0.1",
    "core-js": "^2.6.5",
    "rxjs": "6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^7.2.14",
    "@angular-devkit/build-ng-packagr": "~0.6.8",
    "@angular-devkit/build-angular": "~0.6.6",
    "ng-packagr": "^3.0.0-rc.2",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "typescript": "~3.2.2",
    "@angular/cli": "~7.2.2",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1"
  },
  "description": "Angular6 reporting components",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atom-platform/ngx-charts-web.git"
  },
  "keywords": [
    "widgets",
    "reporting",
    "ui"
  ],
  "author": "atom-platform",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/atom-platform/ngx-charts-web/issues"
  },
  "homepage": "https://github.com/atom-platform/ngx-charts-web#readme"
}
